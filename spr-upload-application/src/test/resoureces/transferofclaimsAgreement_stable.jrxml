					<font fontName="宋体" size="12" isBold="false" pdfFontName="STSong-Light" pdfEncoding="UniGB-UCS2-H" isPdfEmbedded="true"/>
				</textElement>
				<text><![CDATA[权”）。]]></text>
			</staticText>
			<staticText>
				<reportElement x="8" y="148" width="540" height="22" uuid="f70eeca8-1839-4f02-ab95-ecfb762091b9"/>
